<view class="box" wx:for="{{businfo}}">
  <view class="box-left" >
      <image src="{{item.spic}}"></image>
  </view>

  <view class="box-right"  >
      <view class="right-a">
        <text>{{item.sname}}</text>
      </view>
      <view class="right-b">
        <text class="b-a">接受预定</text>
        <text class="b-b">10:00后配送</text>
      </view>
      <view class="right-c">
        <image src="/images/星星.png"></image>
        <text>{{item.sping}}     月售{{item.sxiao}}</text>
      </view>
      <view class="right-d">
        <text>起送¥15    免费配送</text>
      </view>
      <view class="right-e">
        <text>遂宁冒菜人气飙升第一名</text>
      </view>
      <view class="right-f">
        <text>20减2</text>
        <text>40减4</text>
        <text>100减10</text>
        <text>首单减5</text>
      </view>
   </view>

</view>